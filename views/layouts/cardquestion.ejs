<% if(findQuestion != ''){ %> <% findQuestion.forEach(quiz => { %>
<div class="col-12 col-md-6 col-lg-4 my-2">
  <div class="card">
    <div class="card-body">
      <h5 class="card-title"><%=quiz.title %></h5>
      <p class="card-text"><%=quiz.description %></p>
      <a href="/create/<%=quiz._id%>" class="btn btn-secondary">Edit Quiz</a>
      <a href="#" class="btn btn-primary">Start Quiz</a>
    </div>
  </div>
</div>
<% }) %> <%} else {%>
<h6 class="alert alert-danger m-3" style="width: auto">No quiz yet!</h6>
<%}%>
